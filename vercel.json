{
    "builds": [
        {
            "src": "CRUD/wsgi.py",
            "use": "@vercel/phyton",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.10"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "CRUD/wsgi.py"
        }
    ]
}